#simple alarm using python

import time

def alarm_clock():
    alarm_time=input("Enter the alarm time(HH:MM) 24 hours format:")
    print(f"alarm set for {alarm_time}..")

    while True:
      current_time=time.strftime("%H:%M")
      if current_time == alarm_time:
          print("it's time to wake up")
          break

alarm_clock()
